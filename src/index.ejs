<!DOCTYPE html>
<html lang="bg">
    <head>
        <% if(locals.headTags) { %>
            <%- headTags.title.toString() %>
            <%- headTags.meta.toString() %>
        <% } else { %>
            <title>Ти броиш</title>
        <% } %>
        <meta charset="utf-8" />
        <!-- Generated by https://realfavicongenerator.net/-->
        <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
        <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
        <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
        <link rel="manifest" href="/site.webmanifest">
        <meta name="msapplication-TileColor" content="#da532c">
        <meta name="theme-color" content="#ffffff">
        <!-- END FAVICON -->
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
        <meta name="theme-color" content="#000000" />
        <meta name="description" content=""/>
        <link rel="stylesheet" href="/css/normalize.min.css"/>
        <% if(locals.linkTags) { %><%- linkTags %><% } %>
        <% if(locals.styleTags) { %><%- styleTags %><% } %>
        <% if(process.env.NODE_ENV === 'production') { %>
            <script async src="https://www.googletagmanager.com/gtag/js?id=G-7M7P5VJHD1"></script>
            <script>
                window.dataLayer = window.dataLayer || [];
                function gtag(){dataLayer.push(arguments);}
                gtag('js', new Date());

                gtag('config', 'GA_TRACKING_ID', { 'anonymize_ip': true });

                gtag('config', 'G-7M7P5VJHD1');
                gtag('config', 'UA-3011692-43');
            </script>
        <% } %>
    </head>
    <body>
        <% if(locals.staticProps) { %>
            <script>
                window.__INITIAL_DATA__ = <%- JSON.stringify(staticProps) %>;
            </script>
        <% } else { %>
            <script>
                window.__INITIAL_DATA__ = {};
            </script>
        <% } %>
        <div id="root"><% if(locals.body) { %><%- body %><% } %></div>
        <% if(locals.scriptTags) { %>
            <%- scriptTags %>
        <% } else { %>
            <script src="/bundles/bundle.js"></script>
        <% } %>
        <script defer src="https://www.instagram.com/embed.js"></script>
        <div id="fb-root"></div>
        <script async defer crossorigin="anonymous" src="https://connect.facebook.net/en_US/sdk.js#xfbml=1&version=v9.0&appId=0&autoLogAppEvents=1" nonce="IeSXLymW"></script>         
    </body>
</html>